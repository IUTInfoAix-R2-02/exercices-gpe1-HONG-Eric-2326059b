<?xml version="1.0" encoding="UTF-8"?>

<?import javafx.geometry.Insets?>
<?import javafx.scene.control.Button?>
<?import javafx.scene.control.PasswordField?>
<?import javafx.scene.control.TextField?>
<?import javafx.scene.layout.GridPane?>

<GridPane fx:controller="fr.amu.iut.exercice5.LoginControl"
          xmlns:fx="http://javafx.com/fxml" alignment="CENTER" hgap="10" vgap="10">
    <TextField fx:id="userId" GridPane.columnIndex="1" GridPane.rowIndex="0"/>
    <PasswordField fx:id="pwd" GridPane.columnIndex="1" GridPane.rowIndex="1"/>
    <Button fx:id="okButton" text="OK" onAction="#okClicked" GridPane.columnIndex="1" GridPane.rowIndex="2"/>
    <Button fx:id="cancelButton" text="Cancel" onAction="#cancelClicked" GridPane.columnIndex="2" GridPane.rowIndex="2"/>
</GridPane>